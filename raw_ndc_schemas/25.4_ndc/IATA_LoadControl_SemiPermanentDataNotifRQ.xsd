			<xs:element name="SumOfSquareInd" type="IndType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that curtailment value is to be applied to the CG as a total value (false) or as a component of the quadratic sum (true).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightTypeCode" type="WeightTypeCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Type of weight of CG limits to which the curtailment must be applied: Taxi (TA), Take Off (TO), Landing (LD) , Zero Fuel (ZF), In-flight (IN).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="DateTimeType">
		<xs:annotation>
			<xs:documentation>A date time identifies a date and time of day to various common resolutions: year, month, week, day, hour, minute, second, and fraction of second.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="DeckCodeContentType">
		<xs:annotation>
			<xs:documentation>Enumeration set for the Deck Code BDT. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Lower">
				<xs:annotation>
					<xs:documentation>Lower Deck</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Main">
				<xs:annotation>
					<xs:documentation>Main Deck</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Upper">
				<xs:annotation>
					<xs:documentation>Upper Deck</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DeckCodeType">
		<xs:annotation>
			<xs:documentation>Specifies a Deck Code.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="DeckCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="DeckType">
		<xs:annotation>
			<xs:documentation>A structural floor level. For aircraft having one structural level only, this floor level shall be referred to as the main deck. For aircraft having more than one structural floor level, the different floor levels shall be referred to as 'lower deck', 'main deck' and 'upper deck', starting from bottom to top.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceArmAftMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the aft edge of the hold.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmForwardMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the forward edge of the hold.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinComponent" type="CabinComponentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Non-seat related cabin feature or facility (e.g. lavatory, galley, closet, stowage, etc.)
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinCrewLocation" type="CabinCrewLocationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Seat location for cabin crew.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinSection" type="CabinSectionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Division of the cabin into zones for the purpose of balance.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeckCode" type="DeckCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Aircraft deck code (Upper, Main, Lower).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Hold" type="HoldType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A space in an aircraft confined by ceiling, floor, walls and bulkhead, used for carrying load.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmFromMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the left hand edge of the hold. Enter values as negative.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmToMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the right hand edge of the hold. Enter values as positive.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaximumWeightMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Maximum total weight permitted on the deck</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VolumeMeasure" type="VolumeMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total volume available on the deck</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="DoorPositionCodeContentType">
		<xs:annotation>
			<xs:documentation>Enumeration set for the Door Position Code BDT. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:length value="1"/>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Center of aircraft</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L">
				<xs:annotation>
					<xs:documentation>Left side of aircraft</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation>Right side of aircraft</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DoorPositionCodeType">
		<xs:annotation>
			<xs:documentation>Specifies Door Position codes.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="DoorPositionCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="DoorType">
		<xs:annotation>
			<xs:documentation>A swinging or sliding barrier that will close the entrance to a room or building or vehicle.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceArmAftMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Linear dimension from the aircraft datum to the aft edge of the door.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmForwardMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Linear dimension from the aircraft datum to the forward edge of the door.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DoorID" type="ID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a door within a hold. E.g. Door 1, Door 2, Door 1L, Door 2R.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HeightMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Linear dimension of the height of the door.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PositionCode" type="DoorPositionCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Position of the door; L = Left side of aircraft, R = Right side of aircraft, C = Center of aircraft</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DOW_ConfigurationType">
		<xs:annotation>
			<xs:documentation>Identification of operational items such as crew, pantry, potable water and/or service weight adjustments and their weight and/or index influence that are included in the Dry Operating Weight of an aircraft or aircraft group.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewBaggages" type="CrewBaggagesType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Crew Baggage included in the DOW configuration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewDistributionCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name/identifier of the crew distribution, e.g. 'A', 'International Ops', 'Domestic'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DefaultIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Default Dry Operating Index that may apply for all aircraft registrations of a group.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DefaultMAC_Percent" type="PercentType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Default Dry Operating %MAC that may apply for all aircraft registrations of a group.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DefaultWeightMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Default Dry Operating Weight that may apply for all aircraft registrations of a group.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DOW_ConfigurationCode" type="CodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name/identifier of the Dry Operating Weight Configuration, e.g. 'All UK Services', 'B'. 'Flight Range 1'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncludedCabinCrewTotal" type="NumberType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total number of Cabin Crew Included in the Dry Operating Weight Configuration.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncludedCrewBaggageTotal" type="NumberType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total number of Crew Baggage's included in the DOW Configuration.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncludedFlightDeckCrewTotal" type="NumberType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total number of Flight Deck Crew Included in the Dry Operating Weight Configuration.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PantryDistributionCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name/identifier of the pantry distribution, e.g. 'Domestic', 'Charter', 'Z'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PotableWaterDistribution" type="PotableWaterDistributionType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Distribution of potable water to each individually specified potable water tank or to all tanks.
 Water can be specified as follows:
 - Weight per tank, with corresponding index.
 - Distribution: For a given total water weight, specify the quantity in each tank
 - Specific water weights, with corresponding index.
 - Adjustment (from full) e.g. full water included in basic weight.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PotableWaterDistributionCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name/identifier of the potable water distribution, e.g. 'Long Haul', 'Short Haul', '75% Water', '1/4','1/2','3/4','0','200'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SWA" type="SWA_Type" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Service Weight Adjustment. Standard adjustment to the service weight of an aircraft with optional balance arm and/or index.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SWA_Code" type="CodeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Name/identifier of the Service Weight Adjustment, e.g. 'FS1', 'Return Catering'.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="DOW_ItemCodeContentType">
		<xs:annotation>
			<xs:documentation>Enumeration set for Dry Operating Weight Item codes (Basic Weight, Flight Deck Crew, Cabin Crew, Flight Deck Crew Baggage, Cabin Crew Baggage, Pantry, Potable water...). {codeListAgencyName}IATA{/codeListAgencyName} {codeListName}Items for Dry Operating Weight/Index{/codeListName} {codeListIdentifier}PADIS DOW{/codeListIdentifier} </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="DOW_ItemCodeType">
		<xs:annotation>
			<xs:documentation>Specifies Dry Operating Weight Item codes (Basic Weight, Flight Deck Crew, Cabin Crew, Flight Deck Crew Baggage, Cabin Crew Baggage, Pantry, Potable water...) based on PADIS Code List.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="DOW_ItemCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="DOW_ItemType">
		<xs:annotation>
			<xs:documentation>Item that can be considered as part of the Dry Operating Weight buildup of an aircraft or aircraft group as per company specifications e.g. Basic Weight, Flight Deck Crew, Cabin Crew, Flight Deck Crew Baggage, Cabin Crew Baggage, Pantry, Containers, Pallets, Potable Water, Library, ...
 Other items are included in the total traffic load weight.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DOW_ItemCode" type="DOW_ItemCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Dry Operating Weight Item code (Basic Weight, Flight Deck Crew, Cabin Crew, Flight Deck Crew Baggage, Cabin Crew Baggage, Pantry, Potable water...) based on PADIS Code List.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DryOperatingAircraftType">
		<xs:annotation>
			<xs:documentation>Aircraft configured according to a given Dry Operating Weight configuration.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AdjustmentInd" type="IndType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that weight and center of gravity data are expressed as adjustments to be applied to the corresponding fleet data or as individual aircraft data.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Aircraft" type="AircraftType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A transport vehicle which is certified as airworthy by a competent aeronautical authority.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AircraftRegistrationID" type="AircraftRegistrationID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The complete alphanumeric identification assigned by the appropriate licensing authority to an individual aircraft. Hyphens contained within the registration shall not be included. E.g. GBOAC, N46573, ZZ123.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The balance arm of the center of gravity of the specific aircraft.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DOW_ConfigurationCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name/identifier of the Dry Operating Weight Configuration, e.g. 'All UK Services', 'B'. 'Flight Range 1'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Basic index or Dry Operating Index of the specific aircraft.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAC_Percent" type="PercentType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>%MAC /RC of the specific aircraft.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightMeasure" type="WeightMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Basic Weight or Dry Operating Weight of the specific aircraft.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="DurationType">
		<xs:annotation>
			<xs:documentation>A duration is the specification of a length of time without a fixed start or end time, expressed in Gregorian calendar time units (Year, Month, Week, or Day) and Hours, Minutes or Seconds</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:duration"/>
	</xs:simpleType>
	<xs:complexType name="EmailAddressType">
		<xs:annotation>
			<xs:documentation>The email address which should be used for contact purposes.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EmailAddressText" type="TextType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The email address which should be used for contact purposes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LabelText" type="TextType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Used to differentiate multiple email addresses of one contact.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightDeckCrewBaggageLocationType">
		<xs:annotation>
			<xs:documentation>Location of Flight Deck Crew Bag included in the DOW configuration</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="HoldCompartmentID" type="HoldCompartmentID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a compartment within a hold (compartment no). As per AHM505, compartments are numbered from 1 to 6. A second figure can indicate a section in a compartment starting from the front to the back and from right to left.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HoldID" type="HoldID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a hold within a deck. Typical values include FWD (forward), AFT, BULK, MAIN (deck).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ULD_LoadPositionID" type="ULD_LoadPositionID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a possible ULD position within the aircraft, using one of the patterns described in AHM505. E.g. A, BR, CDL, E3, 13,22L, 32P, 413, 1112O. May contain compartment number, row position, left-to-right position in a row.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightDeckCrewBaggageType">
		<xs:annotation>
			<xs:documentation>Specification of Flight Deck Crew Baggage included in the DOW Configuration.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BaggagePieces" type="QtyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total number of Flight Deck Crew Bags included in the DOW Configuration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BaggageWeightMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Weight of each Flight Crew Bag included in the DOW Configuration.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Baglocation" type="FlightDeckCrewBaggageLocationType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Location of Flight Deck Crew Bag included in the DOW configuration</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightDeckCrewDistributionElementType">
		<xs:annotation>
			<xs:documentation>A specific allocation in a crew distribution related to flight deck crew.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BaggageLocation" type="ULD_LoadPosition1Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Daughter ABIE of ULD Load Position (ABIE) to display only the selected Business Identifier Fields:
 - ULD Load Position ID
 - Balance Arm Forward Measure
 - Balance Arm Aft Measure</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BaggageWithCrewInd" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>True if baggage is with the crew, false otherwise.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinCrewLocationID" type="CabinCrewLocationID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a cabin crew location, e.g. Door 1L, Over wing Exit, Aft Crew Rest, ...</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewQty" type="QtyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Number of flight deck crew allocated to the associated location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightDeckCrewLocationID" type="FlightDeckCrewLocationID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a flight deck crew location. Typical values include Captain, FO, Observer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeqNumber" type="NaturalNumberType" minOccurs="0" maxOccurs="1">
