	<xs:complexType name="SaleableConfigurationSectionClassInfoType">
		<xs:annotation>
			<xs:documentation>Cabin section information of a saleable configuration by cabin type.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CabinTypeCode" type="CabinTypeCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Cabin Type Code as defined by PADIS code set (e.g. 1, 2, 3, etc.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatsQty" type="QtyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Number of seats in the specified cabin section and class.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SaleableConfigurationSectionType">
		<xs:annotation>
			<xs:documentation>Cabin section information of a saleable configuration.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceArmAftMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the aft edge of the specified cabin section. Used when graphical representation of each cabin section is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmCentroidMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the mean point at which the CG influence of all passengers in the cabin section is calculated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmForwardMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the forward edge of the specified cabin section. Used when graphical representation of each cabin section is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinSectionID" type="CabinSectionID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a cabin section within a deck. As per AHM505, cabin sections can be identified starting with a zero (0) as a first figure to indicate the cabin followed by an alpha character starting with A from the front to the rear. If the cabin sections correspond with the configuration layout per class, it is also possible to use the class designator, e.g. F for first class, to identify a cabin section.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SaleableConfigurationSectionClassInfo" type="SaleableConfigurationSectionClassInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Cabin section information of a saleable configuration by cabin type.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatsQty" type="QtyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total number of seats in the specified cabin section.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightUnitIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The index influence of one weight unit (as defined under aircraft units) of passenger weight within the specified cabin section, assumed to be located at the mean centroid of passenger seats within the cabin section.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SaleableConfigurationType">
		<xs:annotation>
			<xs:documentation>Planned utilization layout of aircraft cabin section/zone that is fit or able to be sold.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SaleableConfigurationClassInfo" type="SaleableConfigurationClassInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Saleable configuration information by cabin type.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SaleableConfigurationID" type="ID_Type" minOccurs="1" maxOccurs="1">
