<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2024 rel. 2 sp1 (x64) (https://www.altova.com) by Cornu (Iata) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.iata.org/IATA/2015/00/2025.2/IATA_LoadControl_SemiPermanentDataNotifRQ" targetNamespace="http://www.iata.org/IATA/2015/00/2025.2/IATA_LoadControl_SemiPermanentDataNotifRQ" elementFormDefault="qualified" version="1.009">
	<xs:element name="IATA_LoadControl_SemiPermanentDataNotifRQ">
		<xs:annotation>
			<xs:documentation>Used to exchange semi-permanent data for weight and balance between the carrier owning the aircraft and airlines that operate the aircraft, handling agents and DCS providers.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Remark</xs:documentation>
			</xs:annotation>
			<xs:sequence minOccurs="0" maxOccurs="1">
				<xs:element name="Carrier" type="CarrierType" minOccurs="1" maxOccurs="99">
					<xs:annotation>
						<xs:documentation>Carrier</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Remark" type="RemarkType" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Remarks</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="AircraftGroupConfigurationType">
		<xs:annotation>
			<xs:documentation>Specific Configuration of an Aircraft Group.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AircraftRegistrationID" type="AircraftRegistrationID_Type" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The complete alphanumeric identification assigned by the appropriate licensing authority to an individual aircraft. Hyphens contained within the registration shall not be included. E.g. GBOAC, N46573, ZZ123.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AsymmetricalLoadLimitationTable" type="AsymmetricalLoadLimitationTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related asymmetrical load limitations.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceOutputRequirement" type="BalanceOutputRequirementType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Requirement to put specific balance information on specific types of load sheet (provisional, final, ACARS,...).
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CargoHeatingDevice" type="CargoHeatingDeviceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Heating capability of the cargo hold.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CargoVentilationDevice" type="CargoVentilationDeviceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Ventilation capability of the cargo hold.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CenterOfGravityLimitTable" type="CenterOfGravityLimitTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related Center of Gravity (CG) limits.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CombinedLoadLimitationTable" type="CombinedLoadLimitationTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related combined load limitations.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewDistribution" type="CrewDistributionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Allocation of crew (flight deck and cabin crew) and their baggage to associated locations.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CumulativeLoadLimitationTable" type="CumulativeLoadLimitationTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related cumulative load limitations.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurtailmentTable" type="CurtailmentTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of Curtailments.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Deck" type="DeckType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A structural floor level. For aircraft having one structural level only, this floor level shall be referred to as the main deck. For aircraft having more than one structural floor level, the different floor levels shall be referred to as 'lower deck', 'main deck' and 'upper deck', starting from bottom to top.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DOW_Configuration" type="DOW_ConfigurationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Identification of operational items such as crew, pantry, potable water and/or service weight adjustments and their weight and/or index influence that are included in the Dry Operating Weight of an aircraft or aircraft group.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DOW_Item" type="DOW_ItemType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Item that can be considered as part of the Dry Operating Weight buildup of an aircraft or aircraft group as per company specifications e.g. Basic Weight, Flight Deck Crew, Cabin Crew, Flight Deck Crew Baggage, Cabin Crew Baggage, Pantry, Containers, Pallets, Potable Water, Library, ...
 Other items are included in the total traffic load weight.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DryOperatingAircraft" type="DryOperatingAircraftType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Aircraft configured according to a given Dry Operating Weight configuration.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FleetBalanceArmMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard empty balance arm that may be utilized for all aircraft registrations of a group.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FleetIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard empty index that may be utilized for all aircraft registrations of a group.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FleetMAC_Percent" type="PercentType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard empty %MAC that may be utilized for all aircraft registrations of a group.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FleetWeightMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard empty weight that may be utilized for all aircraft registrations of a group.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightDeckCrewLocation" type="FlightDeckCrewLocationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Seat location for flight deck crew.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FloorLoadLimitationTable" type="FloorLoadLimitationTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related floor load limitations.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FuelDistribution" type="FuelDistributionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A procedure that defines the consecutive steps for fueling an aircraft and the quantities to distribute to each of the corresponding tanks at each step in case a fuel split calculation is required to be performed by the departure control system.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FuelingProcedure" type="FuelingProcedureType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A procedure for filling the complete set of tanks (All Tanks) of an aircraft or an individual fuel tank, which might be composite when multiple tanks have been combined under one. 
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdealTrimLineTable" type="IdealTrimLineTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related Ideal Trim Line Points forming an ideal trim line applying to a specific set of CG limits or to all CG limits.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralImbalanceLimitTable" type="LateralImbalanceLimitTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related Lateral Imbalance Limits.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaximumWeightsTable" type="MaximumWeightsTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of maximum certified weights.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MinimumWeightsTable" type="MinimumWeightsTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of minimum certified weights.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OperatingCarrier" type="Carrier1Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Daughter ABIE of Carrier (ABIE) to display only the selected Business Identifier Fields:
 - Airline Designator Code
 - Duplicate Designator Indicator
 - Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PantryDistribution" type="PantryDistributionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Allocation of galley equipment, food, service items etc. to specified locations or regardless of location.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaxTrimOutputRequirement" type="PaxTrimOutputRequirementType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Requirement to put specific passenger trim output on the load sheet.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PotableWaterDistribution" type="PotableWaterDistributionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Distribution of potable water to each individually specified potable water tank or to all tanks.
 Water can be specified as follows:
 - Weight per tank, with corresponding index.
 - Distribution: For a given total water weight, specify the quantity in each tank
 - Specific water weights, with corresponding index.
 - Adjustment (from full) e.g. full water included in basic weight.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PotableWaterTank" type="PotableWaterTankType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tank of potable water located in the cabin.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RunningLoadLimitationTable" type="RunningLoadLimitationTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related running load limitations.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SaleableConfiguration" type="SaleableConfigurationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Planned utilization layout of aircraft cabin section/zone that is fit or able to be sold.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STAB_Table" type="STAB_TableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related stabilizer trim settings for given flap settings or thrust ratings.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartWeightTypeCode" type="StartWeightTypeCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Method of starting weight calculation (Basic Weight (BW), Dry Operating Weight (DOW)).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupplementaryInfoRequirement" type="SupplementaryInfoRequirementType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Requirement to add specific data to the supplementary information (SI) area of each applicable load sheet (provisional, final, ACARS,...).
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SWA" type="SWA_Type" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Service Weight Adjustment. Standard adjustment to the service weight of an aircraft with optional balance arm and/or index.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TaxiFuelProcedure" type="TaxiFuelProcedureType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tank sequence for fuel burn and fuel quantity used during the taxiing of an aircraft depending on certain conditions (Standard, Training Flights, etc. ...).
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TippingLimitTable" type="TippingLimitTableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of related Tipping Limits.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AircraftGroupType">
		<xs:annotation>
			<xs:documentation>Group of aircraft operating together under the same ownership, belonging to the same aircraft type and having a set of configuration data in common.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Aircraft" type="AircraftType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A transport vehicle which is certified as airworthy by a competent aeronautical authority.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AircraftGroupConfiguration" type="AircraftGroupConfigurationType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Specific Configuration of an Aircraft Group.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AircraftName" type="NameType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Internal name given by the carrier to this aircraft type. Aircraft type as it appears on the load sheet.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AircraftSubtypeName" type="NameType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Series or subtype also referred to as suffix in the IATA SIM manual.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AreaUnitCode" type="AreaUnitCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Area unit used for aircraft calculation. When missing square of length unit is used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FuelDensityVolumeUnitCode" type="LiquidVolumeUnitCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Fuel density liquid volume unit used for aircraft calculation. When missing standard aircraft liquid volume unit is used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FuelDensityWeightUnitCode" type="WeightUnitCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Fuel density weight unit used for aircraft calculation. When missing standard aircraft weight unit is used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IATA_AircraftTypeCode" type="IATA_AircraftTypeCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Code assigned to an aircraft type in IATA SSIM.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndexConversionConstantValue" type="ValueType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Constant (C) used as denominator in the index calculation formula to convert moment values expressed in aircraft moment units into index values.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndexOffsetConstantValue" type="ValueType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Constant (K) used in the index calculation formula as a plus value to avoid negative index figures.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LeadingEdgeMeasure" type="LengthMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Horizontal distance in aircraft length units from the reference datum to the location of the Leading Edge.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LengthUnitCode" type="LengthUnitCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Length unit used for aircraft calculation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LiquidVolumeUnitCode" type="LiquidVolumeUnitCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Liquid volume unit used for aircraft calculation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAC_Measure" type="LengthMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Length of Mean Aerodynamic Chord/ Reference Chord (RC) in aircraft length units.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ManufacturerName" type="NameType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name of the aircraft manufacturer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MomentLengthUnitCode" type="LengthUnitCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Moment length unit used for aircraft calculation. When missing standard aircraft length unit is used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MomentWeightUnitCode" type="WeightUnitCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Moment weight unit used for aircraft calculation. When missing standard aircraft weight unit is used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RefArmMeasure" type="LengthMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Selected Station around which all index values are calculated expressed in aircraft length units from the aircraft reference datum.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VolumeUnitCode" type="VolumeUnitCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Volume unit used for aircraft calculation. Overrides unit defined against the carrier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightUnitCode" type="WeightUnitCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Weight unit used for aircraft calculation. Overrides unit defined against the carrier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AircraftRegistrationID_Type">
		<xs:annotation>
			<xs:documentation>The complete alphanumeric identification assigned by the appropriate licensing authority to an individual aircraft. Hyphens contained within the registration shall not be included. E.g. G-BOAC, N46573, ZZ123.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:maxLength value="10"/>
			<xs:minLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AircraftType">
		<xs:annotation>
			<xs:documentation>A transport vehicle which is certified as airworthy by a competent aeronautical authority.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AircraftRegistrationID" type="AircraftRegistrationID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The complete alphanumeric identification assigned by the appropriate licensing authority to an individual aircraft. Hyphens contained within the registration shall not be included. E.g. GBOAC, N46573, ZZ123.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MSN_ID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Manufacturer's Serial Number. Unique ID assigned at the time of the manufacture of the aircraft and remains constant regardless of the civil registration.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A name that may be applied by an operator to a specific aircraft</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>An identifier utilized by an operator other than registration. This may be an abbreviated registration (e.g. last two characters) and may be painted on the external surface of the aircraft such as the tip of the tail fin, nose landing gear doors etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RegistrationGroupCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>In certified documentation individual or groups of aircraft may be expressed in common sections. This is usually specified in the page header or footer and cross refers to an effective page.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AirflowRateBaseCodeContentType">
		<xs:annotation>
			<xs:documentation>Enumeration set for the Airflow Rate Base Code BDT. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:length value="1"/>
			<xs:enumeration value="H">
				<xs:annotation>
					<xs:documentation>Per Hour</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="M">
				<xs:annotation>
					<xs:documentation>Per Minute</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirflowRateBaseCodeType">
		<xs:annotation>
			<xs:documentation>Specifies Airflow Rate Base code list (H (per Hour), M (per Minute)).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="AirflowRateBaseCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="AirlineCabinTypeType">
		<xs:annotation>
			<xs:documentation>Airline specific information related to the standard 'Cabin Types' or subset of the latter utilized by the airline.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CabinTypeCode" type="CabinTypeCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Cabin Type Code as defined by PADIS code set (e.g. 1, 2, 3, etc.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Desc" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Clarification of the Cabin Type code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PriorityNumber" type="NaturalNumberType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The highest to lowest class where a departure control system automatically re-grades passengers to accommodate over-sales during the load planning process.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AirlineDesigCodeContentType">
		<xs:annotation>
			<xs:documentation>The codelist contents of IATA Resolution 762 airline codes, or of ICAO airline codes. {codeListAgencyName}IATA or ICAO{/codeListAgencyName} {codeListName}Resolution 762 Airline Codes or ICAO Airline Codes{/codeListName} </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:pattern value="([A-Z]{3}|[A-Z]{2})|([0-9][A-Z])|([A-Z][0-9])"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirlineDesigCodeType">
		<xs:annotation>
			<xs:documentation>Either the IATA-defined 2-character code or the ICAO-defined 3-character code of an airline, as per the length of the value.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="AirlineDesigCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="AirlineLoadInfoType">
		<xs:annotation>
			<xs:documentation>Information distinguishing different categories of cargo used for load planning characterized by Airline specific codes in addition to codes defined in AHM510.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Desc" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of the airline specific load information code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoadInfoCode" type="CodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Airline specific load information code.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AreaUnitCodeContentType">
		<xs:annotation>
			<xs:documentation>List of Area Unit Codes. Subset of UN/CEFACT Recommendation 20. {codeListAgencyName}AIDM (extract from UN/CEFACT){/codeListAgencyName} </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="CMK">
				<xs:annotation>
					<xs:documentation>Square Centimeter</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FTK">
				<xs:annotation>
					<xs:documentation>Square Foot</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INK">
				<xs:annotation>
					<xs:documentation>Square Inch</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MTK">
				<xs:annotation>
					<xs:documentation>Square Meter</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AreaUnitCodeType">
		<xs:annotation>
			<xs:documentation>Specifies Area Unit code list.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="AreaUnitCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="AsymmetricalLoadLimitationTableType">
		<xs:annotation>
			<xs:documentation>Set of related asymmetrical load limitations.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AsymmetricalLoadLimitation" type="AsymmetricalLoadLimitationType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Maximum load acceptable on each side of the compartment floor in relation with each other.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConditionText" type="TextType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Criteria for which the table of load limitations must be applied, e.g. Tail Tank in use, Aft ZFCG.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LinearLoadInd" type="IndType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicator of the nature of the limitations relationship i.e. Linear Load or Weight.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeFromCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The starting value of a range for which the specific table applies. Might be numeric in case range type 'balance arm' or alphanumeric in case of range type 'ULD position'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeToCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The end value of a range for which the specific table applies. Might be numeric in case range type 'balance arm' or alphanumeric in case of range type 'ULD position'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeTypeCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Type or nature of the defined From/To values that limit the applicability of the load limitation table. This may be 'ULD Positions' or 'Balance Arm'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TableName" type="NameType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specific name for the table of structural load limitations.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AsymmetricalLoadLimitationType">
		<xs:annotation>
			<xs:documentation>Maximum load acceptable on each side of the compartment floor in relation with each other.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LeftSideValue" type="ValueType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The maximum value permitted on the left side in relation to the value specified against Right Side. The nature of the relationship (Weight or Linear Load) is a property of the table containing the asymmetrical load limitation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RightSideValue" type="ValueType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The maximum value permitted on the right side in relation to the value specified against Left Side. The nature of the relationship (Weight or Linear Load) is a property of the table containing the asymmetrical load limitation.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BaggageStandardWeightsType">
		<xs:annotation>
			<xs:documentation>Approved baggage standard weights to be used for weight and balance calculation.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BagsPerPaxQty" type="QtyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Average number of bags of checked baggage expected per passenger</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BagVolumeMeasure" type="VolumeMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Average bag volume of checked baggage</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BagWeightPerPaxMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Average bag weight of checked baggage per passenger</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinTypeCode" type="CabinTypeCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Cabin Type Code as defined by PADIS code set (e.g. 1, 2, 3, etc.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VariationCode" type="VariationForWeightsCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Variation code. Apart from Standard may include Domestic, International, Charter, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightPerBagMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard weight of checked baggage per bag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightPerPaxMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard weight of checked baggage per passenger</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="BalanceOutputCodeContentType">
		<xs:annotation>
			<xs:documentation>Enumeration set for Balance Output codes (Basic Index, Dry Operating Index, Dead load MAC, MAC - at zero fuel weight,...) {codeListAgencyName}IATA{/codeListAgencyName} {codeListName}Balance output{/codeListName} {codeListIdentifier}PADIS BAL{/codeListIdentifier} </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="BalanceOutputCodeType">
		<xs:annotation>
			<xs:documentation>Specifies Balance Output codes (Basic Index, Dry Operating Index, Dead load MAC, MAC - at zero fuel weight,...) based on PADIS Code List.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="BalanceOutputCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="BalanceOutputRequirementType">
		<xs:annotation>
			<xs:documentation>Requirement to put specific balance information on specific types of load sheet (provisional, final, ACARS,...).
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceOutputCode" type="BalanceOutputCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Balance Output code (Basic Index, Dry Operating Index, Dead load MAC, MAC - at zero fuel weight,...) based on PADIS Code List.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinalACARS_Ind" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that output is required on the final ACARS load sheet (AHM518).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinalInd" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that output is required on the final EDP load sheet (AHM517).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreliminaryACARS_Ind" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that output is required on the preliminary ACARS load sheet (AHM518).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreliminaryInd" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that output is required on the preliminary EDP load sheet (AHM517).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="BayGroupingID_Type">
		<xs:annotation>
			<xs:documentation>Specifies a Bay Grouping identifier.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="11"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BayGroupingType">
		<xs:annotation>
			<xs:documentation>A grouping of ULD positions (Stack/String) used as a method of maximizing the ULD capacity of a lower hold by not using intermediate locks.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceArmAftMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the aft edge of the Bay Grouping. Used when graphical representation of each deck is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmForwardMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the forward edge of the Bay Grouping. Used when graphical representation of each deck is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BayGroupingID" type="BayGroupingID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a Bay (ULD) Grouping (String/Stack) in an aircraft. As per AHM565 Bay Groupings can be identified by joining the identifiers of the first and last bay in the group separated by a hyphen, e.g. 12L-21L. The Grouping ID can also contain configuration information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ColorCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The color coding utilized to identify a specific ULD grouping or configuration.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmCentroidMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Used where lateral imbalance calculations are required for the aircraft type. Specify values as negative for linear dimensions extending to the left of the aircraft centerline and positive for linear dimensions extending to the right of the aircraft centerline.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmFromMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the left hand edge of the Bay Grouping. Enter values as negative.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmToMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the right hand edge of the Bay Grouping. Enter values as positive.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaximumWeightMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Maximum permitted weight for the Bay Grouping</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MissingRestraintRule" type="MissingRestraintRuleType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Restriction on weight applicable when one or more locks are inoperable or missing.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ULD_LoadPosition" type="ULD_LoadPositionType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A subdivision of a containerized/palletized compartment.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BayLockType">
		<xs:annotation>
			<xs:documentation>Lock used by a Bay Grouping or a ULD Load Position.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BayGroupingID" type="BayGroupingID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a Bay (ULD) Grouping (String/Stack) in an aircraft. As per AHM565 Bay Groupings can be identified by joining the identifiers of the first and last bay in the group separated by a hyphen, e.g. 12L-21L. The Grouping ID can also contain configuration information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Lock" type="LockType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>ULD restraint which can be fixed within the aircraft.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LockPositionCode" type="LockPositionCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Position of lock relative to the ULD position.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CabinComponentType">
		<xs:annotation>
			<xs:documentation>Non-seat related cabin feature or facility (e.g. lavatory, galley, closet, stowage, etc.)
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceArmAftMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the aft edge of the specified cabin location. Used when graphical representation of each specified cabin location is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmCentroidMeasure" type="LengthMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the specified cabin location, at which the departure control system will calculate the center of gravity influence of load in that location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmForwardMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the forward edge of the specified cabin location. Used when graphical representation of each specified cabin location is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinComponentName" type="NameType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name or description of the cabin component, e.g. Forward Galley, First Class Coat Locker.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinComponentTypeCode" type="CabinComponentTypeCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Code of the cabin component Type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmCentroidMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The lateral position of the specified cabin location. Used when cabin load lateral imbalance influence is required to be calculated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmFromMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the left hand edge of the specified cabin location. Used when the graphical representation of each specified cabin location is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmToMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the right hand edge of the specified cabin location. Used when the graphical representation of each specified cabin location is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaximumWeightMeasure" type="WeightMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Maximum weight permitted in the specified cabin location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightUnitIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The index influence of one weight unit (as defined under aircraft units) of load located at that location.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CabinComponentTypeCodeContentType">
		<xs:annotation>
			<xs:documentation>Cabin component type code set.
 Examples:
 701 (Movable compartment divider)
 702 (Convertible seats)
 AR (Air phone)
 BA (Bar)
 BK (Bulkhead)
 CL (Closet)
 G (Galley)
 LA (Lavatory)
 LG (Luggage storage)
 MV (Movie screen)
 SO (Storage space)
 ST (Stairs to upper deck)
 TA (Table {codeListAgencyName}PADIS{/codeListAgencyName} {codeListName}Cabin facilities characteristic{/codeListName} {codeListIdentifier}9978{/codeListIdentifier} </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CabinComponentTypeCodeType">
		<xs:annotation>
			<xs:documentation>Specifies a Cabin Component Type Code.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CabinComponentTypeCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="CabinCrewBaggageLocationType">
		<xs:annotation>
			<xs:documentation>Location of Cabin Crew Bag included in the DOW configuration</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="HoldCompartmentID" type="HoldCompartmentID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a compartment within a hold (compartment no). As per AHM505, compartments are numbered from 1 to 6. A second figure can indicate a section in a compartment starting from the front to the back and from right to left.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HoldID" type="HoldID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a hold within a deck. Typical values include FWD (forward), AFT, BULK, MAIN (deck).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ULD_LoadPositionID" type="ULD_LoadPositionID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a possible ULD position within the aircraft, using one of the patterns described in AHM505. E.g. A, BR, CDL, E3, 13,22L, 32P, 413, 1112O. May contain compartment number, row position, left-to-right position in a row.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CabinCrewBaggageType">
		<xs:annotation>
			<xs:documentation>Specification of Cabin Crew Baggage included in the DOW Configuration.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BaggagePieces" type="QtyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total number of Cabin Crew Bags included in the DOW Configuration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BaggageWeightMeasure" type="WeightMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Weight of each Cabin Crew Bag included in the DOW Configuration.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Baglocation" type="CabinCrewBaggageLocationType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Location of Cabin Crew Bag included in the DOW configuration</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CabinCrewDistributionElementType">
		<xs:annotation>
			<xs:documentation>A specific allocation in a crew distribution related to cabin crew.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BaggageLocation" type="ULD_LoadPosition1Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Daughter ABIE of ULD Load Position (ABIE) to display only the selected Business Identifier Fields:
 - ULD Load Position ID
 - Balance Arm Forward Measure
 - Balance Arm Aft Measure</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BaggageWithCrewInd" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>True if baggage is with the crew, false otherwise.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinCrewLocationID" type="CabinCrewLocationID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a cabin crew location, e.g. Door 1L, Over wing Exit, Aft Crew Rest, ...</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewQty" type="QtyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Number of cabin crew allocated to the associated location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightDeckCrewLocationID" type="FlightDeckCrewLocationID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a flight deck crew location. Typical values include Captain, FO, Observer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeqNumber" type="NaturalNumberType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Sequence of seating for determination of crew distribution.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CabinCrewLocationID_Type">
		<xs:annotation>
			<xs:documentation>Specifies a Cabin Crew Location identifier.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CabinCrewLocationType">
		<xs:annotation>
			<xs:documentation>Seat location for cabin crew.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceArmCentroidMeasure" type="LengthMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the cabin crew location, at which the departure control system will calculate the center of gravity influence of crew located at that cabin crew location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinCrewLocationID" type="CabinCrewLocationID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a cabin crew location, e.g. Door 1L, Over wing Exit, Aft Crew Rest, ...</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmCentroidMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The lateral position of the cabin crew location. Used when crew lateral imbalance influence is required to be calculated, and/or when graphical representation of each crew location is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatsQty" type="QtyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The total number of seats that exist at the specified cabin crew location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightUnitIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The index influence of one weight unit (as defined under aircraft units) of crew weight located at that cabin crew location.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CabinSectionID_Type">
		<xs:annotation>
			<xs:documentation>Specifies a Cabin Section identifier.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="2"/>
			<xs:pattern value="[A-Z]|(0[A-Z])"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CabinSectionType">
		<xs:annotation>
			<xs:documentation>Division of the cabin into zones for the purpose of balance.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BalanceArmAftMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the aft boundary of the cabin section. Used when graphical representation of each cabin section is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmCentroidMeasure" type="LengthMeasureType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The mean linear dimension of passenger seats within the cabin section from the aircraft datum, at which the departure control system will calculate the center of gravity influence of passengers located within that cabin section.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceArmForwardMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft datum to the forward boundary of the cabin section. Used when graphical representation of each cabin section is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinSectionID" type="CabinSectionID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a cabin section within a deck. As per AHM505, cabin sections can be identified starting with a zero (0) as a first figure to indicate the cabin followed by an alpha character starting with A from the front to the rear. If the cabin sections correspond with the configuration layout per class, it is also possible to use the class designator, e.g. F for first class, to identify a cabin section.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndRowNumber" type="NaturalNumberType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Seat Row Number at which the cabin ends.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmFromMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the left hand edge of the cabin section. Used when the graphical representation of each cabin section is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LateralArmToMeasure" type="LengthMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The linear dimension from the aircraft centerline to the right hand edge of the cabin section. Used when the graphical representation of each cabin section is available in the supplier's departure control system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeatRow" type="SeatRowType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Row of passenger seats located in the cabin.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartRowNumber" type="NaturalNumberType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Seat Row Number at which the cabin starts.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightUnitIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The index influence of one weight unit (as defined under aircraft units) of passenger weight within the specified cabin section, assumed to be located at the mean centroid of passenger seats within the cabin section.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CabinTypeCodeContentType">
		<xs:annotation>
			<xs:documentation>Enumeration values of Cabin Type Codes as defined by PADIS codeset 9873. {codeListAgencyName}IATA{/codeListAgencyName} {codeListName}PADIS Codeset 9873{/codeListName} </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CabinTypeCodeType">
		<xs:annotation>
			<xs:documentation>Additional BDT to specify the Cabin Type Code as defined by PADIS.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CabinTypeCodeContentType"/>
	</xs:simpleType>
	<xs:complexType name="CargoHeatingDeviceType">
		<xs:annotation>
			<xs:documentation>Heating capability of the cargo hold.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CockpitMasterSwitchInd" type="IndType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that there is a master switch in the cockpit for the heating capability.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HoldCompartmentID" type="HoldCompartmentID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a compartment within a hold (compartment no). As per AHM505, compartments are numbered from 1 to 6. A second figure can indicate a section in a compartment starting from the front to the back and from right to left.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HoldID" type="HoldID_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a hold within a deck. Typical values include FWD (forward), AFT, BULK, MAIN (deck).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SelectionLocationName" type="NameType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The location of the aircraft's cargo heat control panel, e.g. 'Cockpit', 'Forward Hold'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SelectionName" type="NameType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The setting as it appears on the aircraft's cargo heat control panel. Exact wording should be used where possible to avoid confusion, e.g. 'Heat', 'Off'. Leave blank if no temperature control available.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TemperatureFromValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Minimum expected temperature for the specified control panel setting.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TemperatureToValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Maximum expected temperature for the specified control panel setting.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ULD_LoadPositionID" type="ULD_LoadPositionID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a possible ULD position within the aircraft, using one of the patterns described in AHM505. E.g. A, BR, CDL, E3, 13,22L, 32P, 413, 1112O. May contain compartment number, row position, left-to-right position in a row.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CargoVentilationDeviceType">
		<xs:annotation>
			<xs:documentation>Ventilation capability of the cargo hold.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirExchDuration" type="DurationType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Elapsed time taken for the air in the hold or compartment to be completely replenished.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirExchRateValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The flow rate at which the air moves through the hold or compartment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirExchVolumeMeasure" type="VolumeMeasureType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Volume of air in the hold or compartment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirflowRateBaseCode" type="AirflowRateBaseCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that the Air Exchange Rate is expressed in Times Per Hour (true) or Times Per Minute (false).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HoldCompartmentID" type="HoldCompartmentID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a compartment within a hold (compartment no). As per AHM505, compartments are numbered from 1 to 6. A second figure can indicate a section in a compartment starting from the front to the back and from right to left.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HoldID" type="HoldID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a hold within a deck. Typical values include FWD (forward), AFT, BULK, MAIN (deck).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Additional, supplementary information about the document or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SelectionLocationName" type="NameType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The location of the aircraft's cargo ventilation control panel, e.g. 'Cargo Door'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SelectionName" type="NameType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The setting as it appears on the aircraft's cargo ventilation control panel. Exact wording should be used where possible to avoid confusion, e.g. 'High', 'ON'. Leave blank if no ventilation control available.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ULD_LoadPositionID" type="ULD_LoadPositionID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a possible ULD position within the aircraft, using one of the patterns described in AHM505. E.g. A, BR, CDL, E3, 13,22L, 32P, 413, 1112O. May contain compartment number, row position, left-to-right position in a row.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Carrier1Type">
		<xs:annotation>
			<xs:documentation>Daughter ABIE of Carrier (ABIE) to display only the selected Business Identifier Fields:
 - Airline Designator Code
 - Duplicate Designator Indicator
 - Name</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirlineDesigCode" type="AirlineDesigCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Airline code assigned to a carrier. Either ICAO-defined 3-character code or IATA-defined 2-character code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DuplicateDesigInd" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that the IATA Airline Designator is a duplicate code shared by multiple airlines. To identify the airline - in this case - its name must be specified in addition to the designator.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Commercial name of the carrier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CarrierType">
		<xs:annotation>
			<xs:documentation>The organization which carries the passenger, baggage, or goods, and/or commits to delivering the carriage as well as any related services such as issuance of ticket, creation of Order, etc.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AircraftGroup" type="AircraftGroupType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Group of aircraft operating together under the same ownership, belonging to the same aircraft type and having a set of configuration data in common.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirlineCabinType" type="AirlineCabinTypeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Airline specific information related to the standard 'Cabin Types' or subset of the latter utilized by the airline.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirlineDesigCode" type="AirlineDesigCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Airline code assigned to a carrier. Either ICAO-defined 3-character code or IATA-defined 2-character code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirlineLoadInfo" type="AirlineLoadInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information distinguishing different categories of cargo used for load planning characterized by Airline specific codes in addition to codes defined in AHM510.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BaggageStandardWeights" type="BaggageStandardWeightsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Approved baggage standard weights to be used for weight and balance calculation.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CarrierContact" type="ContactInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The electronic or geographic address which a party has provided as the contact channel. For example, contact email, contact postal address, contact phone number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewBaggageStandardWeights" type="CrewBaggageStandardWeightsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Approved crew baggage standard weights to be used for weight and balance calculation.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrewStandardWeights" type="CrewStandardWeightsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Approved crew standard weights to be used for weight and balance calculation.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DuplicateDesigInd" type="IndType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that the IATA Airline Designator is a duplicate code shared by multiple airlines. To identify the airline - in this case - its name must be specified in addition to the designator.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="NameType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Commercial name of the carrier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaxStandardWeights" type="PaxStandardWeightsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Approved passenger standard weights to be used for weight and balance calculation.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ULD_Type" type="ULD_TypeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Grouping of Unit Load Devices (ULD's) with the same dimension, weights and limitations. Is identified by ULD Type Code (eg AKE, PAG, etc.) and optional owner and/or serial number or range of serial numbers.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VolumeUnitCode" type="VolumeUnitCodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard volume unit used by the carrier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WeightUnitCode" type="WeightUnitCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standard weight unit used by the carrier</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CenterOfGravityLimitTableType">
		<xs:annotation>
			<xs:documentation>Set of related Center of Gravity (CG) limits.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the cabin layout (seating configuration) for a given aircraft type e.g. 10C/200Y.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CenterOfGravityLimit" type="CenterOfGravityLimitType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>End point forward or aft of the range within which the center of gravity of an aircraft must lie for safe flight.
 </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CertifiedInd" type="IndType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates that the specified CG limits are those by which the aircraft has been certified (manufacturer's) or that they are operational limits, that have been curtailed in order to account for certain operational assumptions and associated CG calculation inaccuracies.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConditionText" type="TextType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Criteria for which this set of limits should be utilized for a specific flight, e.g. Specific Fuel Range, Aft ZFCG.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeFromValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The minimum value of the range for which the specific table applies.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeToValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The maximum value of the range for which the specific table applies.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeTypeCode" type="CodeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Type or nature of the defined From/To values that limit the applicability of the balance limitation table. This may be specific units e.g. 'KG', 'Liters', but may also be used to identify the range e.g. 'Fuel Volume', 'Zero Fuel MAC'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TableName" type="NameType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specific name for the table of CG limits.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CenterOfGravityLimitType">
		<xs:annotation>
			<xs:documentation>End point forward or aft of the range within which the center of gravity of an aircraft must lie for safe flight.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AftIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Aft CG limit expressed in index units.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AftMAC_Percent" type="PercentType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Aft CG limit expressed as a percentage of the mean aerodynamic chord (MAC).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ForwardIndexValue" type="ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Forward CG limit expressed in index units.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ForwardMAC_Percent" type="PercentType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
