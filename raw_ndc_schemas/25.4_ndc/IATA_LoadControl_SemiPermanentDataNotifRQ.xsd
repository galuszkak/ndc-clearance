		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="TippingLimitTableType">
		<xs:annotation>
			<xs:documentation>Set of related Tipping Limits.
 </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CabinLayoutID" type="ID_Type" minOccurs="0" maxOccurs="1">
