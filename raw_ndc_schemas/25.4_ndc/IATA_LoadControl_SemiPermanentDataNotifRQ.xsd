			<xs:enumeration value="Class trim">
				<xs:annotation>
					<xs:documentation>Class trim</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Seat row">
				<xs:annotation>
					<xs:documentation>Seat row trim</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TrimTypeCodeType">
