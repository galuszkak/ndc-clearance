							<xs:restriction base="xs:NMTOKEN">
								<xs:enumeration value="UPD">
									<xs:annotation>
										<xs:documentation xml:lang="en">Update schedule.</xs:documentation>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="CNL">
									<xs:annotation>
										<xs:documentation xml:lang="en">Cancel schedule.</xs:documentation>
									</xs:annotation>
								</xs:enumeration>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="ConfirmedInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>When true, the schedule is confirmed.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ET_Ind" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>When true this flight in this schedule are ET eligible.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ReleaseDate" type="xs:date" use="optional">
						<xs:annotation>
