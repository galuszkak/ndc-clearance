        </xs:annotation>
      </xs:element>
      <xs:element name="ServiceBundle" type="ServiceBundleType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A set of services offered for sale as a single service.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
