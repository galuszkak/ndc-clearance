        </xs:annotation>
      </xs:element>
      <xs:element name="OrgName" type="NameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The company name of the corporation to whom remittance should be directed. Example: IBM</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
