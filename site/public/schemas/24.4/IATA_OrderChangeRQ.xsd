        </xs:annotation>
      </xs:element>
      <xs:element name="CountrySubDivisionName" type="ProperNameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The name, expressed as text, of the sub-division of a country.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EmailAddressText" type="TextType" minOccurs="1" maxOccurs="1">
