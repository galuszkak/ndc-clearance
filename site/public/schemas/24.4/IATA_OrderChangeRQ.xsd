          <xs:documentation>When related to a Carrier Fee or a Payment Method or a Payment Processing Summary.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Voucher" type="VoucherType" minOccurs="1" maxOccurs="1">
