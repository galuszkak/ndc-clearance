      <xs:documentation>Is the container of all payment relevant functions.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OfferAssociation" type="OfferAssociationType" minOccurs="0" maxOccurs="unbounded">
