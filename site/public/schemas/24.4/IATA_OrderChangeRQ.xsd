    </xs:annotation>
    <xs:restriction base="SeatCharacteristicCodeContentType"/>
  </xs:simpleType>
  <xs:complexType name="SeatItemType">
    <xs:annotation>
      <xs:documentation>Seat service information used for long sells.</xs:documentation>
