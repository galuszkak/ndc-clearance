          <xs:documentation>Corporate ID used for pricing special fares. Example: 675124</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="3" minOccurs="0" name="DifferentialFarePriceType" type="cns:DifferentialFarePriceTypeType">
