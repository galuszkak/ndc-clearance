          <xs:documentation>The name of the redress program (e.g. Secure Flight Program).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="RedressCaseID" type="cns:RedressID_Type">
