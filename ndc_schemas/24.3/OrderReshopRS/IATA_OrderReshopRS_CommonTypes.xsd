          <xs:documentation>Reference to a Passenger ID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PenaltyRefID" type="cns:ID_Type" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Reference to a Penalty ID.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Price" type="cns:DifferentialPriceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A specialization of price used for price differentials. Price is the amount of money expected, required, or given in payment for something.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PricingSystemCodeText" type="cns:TextType" minOccurs="0" maxOccurs="1">
