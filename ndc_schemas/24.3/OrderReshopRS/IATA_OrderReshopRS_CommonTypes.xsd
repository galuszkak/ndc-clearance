        </xs:annotation>
      </xs:element>
      <xs:element name="DisclosureRefID" type="cns:ID_Type" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Uniquely Identifies a disclosure within the context of one message.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Duration" type="cns:DurationType" minOccurs="0" maxOccurs="1">
