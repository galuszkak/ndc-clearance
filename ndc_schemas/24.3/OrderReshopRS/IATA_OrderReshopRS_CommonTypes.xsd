    <xs:sequence>
      <xs:element name="FareDetail" type="cns:DifferentialFareDetailType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>The amount charged by the carrier for the carriage of a passenger and his allowable free baggage.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Price" type="cns:DifferentialPriceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A specialization of price used for price differentials. Price is the amount of money expected, required, or given in payment for something.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DifferentialFareDetailType">
    <xs:annotation>
      <xs:documentation>The amount charged by the carrier for the carriage of a passenger and his allowable free baggage.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AccountCode" type="cns:CodeType" minOccurs="0" maxOccurs="1">
