        </xs:annotation>
      </xs:element>
      <xs:element name="LangCode" type="cns:CodeType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Language code associated with the warning response.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerName" type="cns:ProperNameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The name of the organization owning the warning codelist.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StatusText" type="cns:TextType" minOccurs="0" maxOccurs="1">
