  <xs:complexType name="TaxSummaryType">
    <xs:annotation>
      <xs:documentation>Tax summary information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AllRefundableInd" type="cns:IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>DEPRECATED - This element is deprecated as of 24.1 and will be decommissioned in a future release. Please consult implementation guidance on alternatives to this element.
 When true, all tax breakdown amounts are refundable.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApproximateInd" type="cns:IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this tax is approximate.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollectionInd" type="cns:IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, all specified tax breakdowns must be collected.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GuaranteeTimeLimitDateTime" type="cns:DateTimeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The length of time the Tax of an offer item is guaranteed. In the absence of the Tax guarantee time limit, the Tax is not guaranteed. Upon expiry, the Tax may no longer be guaranteed and either a new shopping process should begin, or the offer should be repriced resulting in a new Offer. 
 Should the Tax guarantee time limit extend beyond Order creation, the remaining length of time persists into the corresponding Order Item. The airline can increase the remaining length of time beyond the original time limit should they wish to.
 This field should only be used when the Airline indicates that a Price Guarantee is available.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RefundMethodText" type="cns:NameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Information from the Airline detailing to the Customer how the tax refund will be processed and what the Customer has to do for the refund.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Tax" type="cns:TaxType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>A compulsory contribution to state revenue, levied by the government on workers' income and business profits or added to the cost of some goods, services, and transactions.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalRefundableTaxAmount" type="cns:AmountType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The total amount of all refundable taxes as calculated by the Airline.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalTaxAmount" type="cns:AmountType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Total of all applicable tax amounts included in the price. If tax breakdowns are provided, this amount should match the sum of the tax breakdown amounts.
 </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TaxType">
    <xs:annotation>
      <xs:documentation>A compulsory contribution to state revenue, levied by the government on workers' income and business profits or added to the cost of some goods, services, and transactions.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AddlFiledTaxCode" type="cns:TaxCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A qualifier to the Tax Code to distinguish taxes/fees/charges with the same tax code but with different business uses.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AddlTaxCode" type="cns:TaxCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A qualifier to the Tax Code to distinguish taxes/fees/charges with the same tax code but with different business uses.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="cns:AmountType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Tax amount. Example: 15000 USD</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApproximateInd" type="cns:IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this tax amount is approximate.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AppTypeCode" type="cns:CodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Indicates at which point the tax was applied (e.g. S - Sale, D - Departure, A - Arrival, X - Delivery).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollectionInd" type="cns:IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this tax must be collected.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollectionPointTax" type="cns:CollectionPointTaxType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Tax data breakdown by collection point for XF(Passenger Facility Fee) and ZP(federal tax segment) taxes.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Country" type="cns:CountryType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A nation with its own government, occupying a particular territory.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountrySubDivision" type="cns:CountrySubDivisionType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>An area of land which is a sub-division within a country, such as a state, a county, a canton, a province, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CurConversion" type="cns:CurConversionType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The result of converting one currency into another currency. E.g. local currency to USD, air miles to monetary.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DescText" type="cns:TextType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Tax short description.
 Example: $4 per flight segment, per passenger (U.S. domestic segment tax)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FiledAmount" type="cns:AmountType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Internal tax filed amount as determined by the airline.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FiledTaxCode" type="cns:TaxCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Internal tax filed type code as determined by the airline.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="QualifierCode" type="cns:TaxQualifierCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Used to specify if current, paid, additional taxes Refer to IATA PADIS Code list data element 5305.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RefundInd" type="cns:IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this tax is refundable.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxCode" type="cns:TaxCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>IATA TTBS Tax Code. Example: AY, GB</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxName" type="cns:NameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The name given to describe the tax. For example, this name can be taken from the IATA TTBS</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TaxTypeCode" type="cns:TaxTypeCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Tax type code (e.g. Applied or Exempt).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
