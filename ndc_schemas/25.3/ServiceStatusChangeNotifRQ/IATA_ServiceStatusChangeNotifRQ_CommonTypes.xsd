          <xs:documentation>Amount added on to a charge for a specific product, purpose, or service.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="LoyaltyUnitAmount" type="cns:LoyaltyAmountType">
