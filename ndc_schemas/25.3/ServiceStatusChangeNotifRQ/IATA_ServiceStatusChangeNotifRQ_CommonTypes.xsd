        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="ValueText" type="cns:TextType">
        <xs:annotation>
          <xs:documentation>Value corresponding to the associated Keyword.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
