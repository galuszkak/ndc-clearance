          <xs:documentation>Primary identifier reflecting the method of pricing for a transaction.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="ReportingCodeText" type="opt:TextType">
        <xs:annotation>
