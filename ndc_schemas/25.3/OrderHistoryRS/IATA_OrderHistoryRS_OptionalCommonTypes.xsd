        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="DepositTimeLimitDateTime" type="opt:DateTimeType">
        <xs:annotation>
          <xs:documentation>The date by which a deposit must be paid for an order item.
 Examples: 2015-01-13T13:59:38Z</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="DisclosureRefID" type="opt:ID_Type">
        <xs:annotation>
          <xs:documentation>References a Disclosure ID.</xs:documentation>
        </xs:annotation>
      </xs:element>
