          <xs:documentation>FOID ID value. Example: NY122345</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="FOID_TypeCode" type="opt:FormOfIdentificationStatusCodeType">
