        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SelectedBundleServicesType">
    <xs:annotation>
      <xs:documentation>For service bundle selection - only to be used for bundles with optional choices (e.g. pick 2 out of 4)</xs:documentation>
    </xs:annotation>
