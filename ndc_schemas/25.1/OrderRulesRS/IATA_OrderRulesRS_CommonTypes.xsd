      <xs:documentation>Touch Point Device Code BDT.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="cns:TouchPointDeviceCodeContentType"/>
  </xs:simpleType>
  <xs:complexType name="TransportPointType">
